{
  "name": "apply-llm-changes-cli",
  "version": "2.1.0",
  "description": "Applies file changes described in LLM output read from stdin.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "apply-llm-changes": "dist/index.js"
  },
  "scripts": {
    "build": "tsc && echo '#!/usr/bin/env node' | cat - dist/index.js > temp && mv temp dist/index.js && chmod +x dist/index.js",
    "dev": "ts-node src/index.ts",
    "prepare": "yarn build",
    "start": "npx @infisical/cli run -- node dist/index.js",
    "test": "npx @infisical/cli run -- jest"
  },
  "keywords": [
    "cli",
    "llm",
    "codegen",
    "automation",
    "typescript",
    "markdown"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "dotenv": "^16.5.0",
    "marked": "^15.0.8",
    "openai": "^4.96.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/marked": "^6.0.0",
    "@types/node": "^20.11.24",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
